<configurator>
  <domain name="CHIP"/>
  <cluster>
    <domain>General</domain>
    <name>Descriptor</name>
    <code>0xF002</code>
    <define>DESCRIPTOR_CLUSTER</define>
    <client init="false" tick="false">true</client>
    <server init="false" tick="false">true</server>
    <description>TODO</description>
    <!-- Currently we do not have yet support for a struct attribute type. The device_type attribute here is a structure. Hence keeping a placeholder for it when it is enabled -->
    <!-- <attribute side="server" code="0x0000" define="DEVICE" type="device_type" optional="false">Device</attribute> -->
    <attribute side="server" code="0x0001" define="SERVER" type="Cluster_Id" min="0x0000" max="0xFFFF" writable="false" default="0x0000" optional="false">Server</attribute>
    <attribute side="server" code="0x0002" define="CLIENT" type="Cluster_Id" min="0x0000" max="0xFFFF" writable="false" default="0x0000" optional="false">Client</attribute>
    <attribute side="server" code="0x0003" define="PARTS" type="Endpoint_Id" min="0x0000" max="0xFFFF" writable="false" default="0x0000" optional="false">Parts</attribute>
  </cluster>
</configurator>
